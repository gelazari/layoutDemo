    padding: 3px;
}

QHeaderView[headerType="stackUpLayerTableHeader"]
{
    background-color: rgba(0,0,0,0);
}

QLabel[labelType="stackUpDescLabel"]
{
    background-color: rgba(0,0,0,0);
    color: #dcdcdc;
    font: bold 12px;
    border: none;
}

QHeaderView[headerType="tableHeader"]:section
{
    background-color: white;
    color: black;
