QLineEdit[fieldType="lineEdit"]
{
    padding-left: 6px;
    padding-right: 6px;
    border: 1px solid #dddddd;
    padding-top: 2px;
    padding-bottom: 2px;
    border-radius: 8px;
}

QComboBox[fieldType="comboBox"]
{
    border: 1px solid #dddddd;
    border-radius: 8px;
    padding-left: 6px;
    padding-right: 6px;
    padding-top: 2px;
    padding-bottom: 2px;
}

QComboBox[fieldType="comboBox"]::drop-down {
    border: none;
}

QComboBox[fieldType="comboBox"]::down-arrow {
    image: url(./images/arrow_down.svg);
    width: 20px;
    height: 20px;
}

